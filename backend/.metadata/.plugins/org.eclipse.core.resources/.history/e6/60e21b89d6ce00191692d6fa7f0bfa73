package com.njh.app.nbacontracts.data;

import java.io.IOException;

import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.select.Elements;

public class PlayerContractData {
	
	private String URL = "https://www.basketball-reference.com";
	
	public Elements getPlayerContractData(String teamAccr) throws IOException {
		Elements playerContractData = null;
        Document document = Jsoup.connect(this.URL).get();
        
        return playerContractData;
	}
}
